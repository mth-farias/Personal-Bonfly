<?xml version="1.0" encoding="utf-8"?>
<WorkflowBuilder Version="2.9.0"
                 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                 xmlns:rx="clr-namespace:Bonsai.Reactive;assembly=Bonsai.Core"
                 xmlns:harp="clr-namespace:Bonsai.Harp;assembly=Bonsai.Harp"
                 xmlns:p1="clr-namespace:Bonsai.Harp.CF;assembly=Bonsai.Harp.CF"
                 xmlns:spk="clr-namespace:Bonsai.Spinnaker;assembly=Bonsai.Spinnaker"
                 xmlns:scr="clr-namespace:Bonsai.Scripting.Expressions;assembly=Bonsai.Scripting.Expressions"
                 xmlns:gl="clr-namespace:Bonsai.Shaders;assembly=Bonsai.Shaders"
                 xmlns:res="clr-namespace:Bonsai.Resources;assembly=Bonsai.System"
                 xmlns:p2="clr-namespace:;assembly=Extensions"
                 xmlns:io="clr-namespace:Bonsai.IO;assembly=Bonsai.System"
                 xmlns:sys="clr-namespace:System;assembly=mscorlib"
                 xmlns:wie="clr-namespace:Bonsai.Windows.Input;assembly=Bonsai.Windows.Input"
                 xmlns:dsp="clr-namespace:Bonsai.Dsp;assembly=Bonsai.Dsp"
                 xmlns:ui="clr-namespace:Bonsai.Design;assembly=Bonsai.Design"
                 xmlns:viz="clr-namespace:Bonsai.Design.Visualizers;assembly=Bonsai.Design.Visualizers"
                 xmlns:gui="clr-namespace:Bonsai.Gui;assembly=Bonsai.Gui"
                 xmlns:p3="clr-namespace:AutomaticGenerators;assembly=Extensions"
                 xmlns:zg="clr-namespace:Bonsai.Gui.ZedGraph;assembly=Bonsai.Gui.ZedGraph"
                 xmlns:cv="clr-namespace:Bonsai.Vision;assembly=Bonsai.Vision"
                 xmlns="https://bonsai-rx.org/2018/workflow">
  <Workflow>
    <Nodes>
      <Expression xsi:type="ExternalizedMapping">
        <Property Name="⚠ LED - COM Port" />
        <Property Name="⚠ Cam - SerialNumber" />
        <Property Name="⚠ Stimulation Screen" />
        <Property Name="⚠ Schema - OptoStim" />
        <Property Name="Schema - VisualStim" />
        <Property Name=".Experiment Duration" />
        <Property Name=".Sampling Rate" />
        <Property Name="0. Save Folder" />
        <Property Name="1. Experimenter" />
        <Property Name="2. Genotype" />
        <Property Name="3. Protocol" />
        <Property Name="4. Sex" />
        <Property Name="5. Age" />
        <Property Name="6. FH Setup" />
      </Expression>
      <Expression xsi:type="GroupWorkflow">
        <Name>Settings</Name>
        <Description>Experiment settings, from hardware to experimental design, and logging.
⚠ Params starting with ⚠ are unique to each setup.</Description>
        <Workflow>
          <Nodes>
            <Expression xsi:type="ExternalizedMapping">
              <Property Name="⚠ LED - COM Port" />
              <Property Name="LED - IR Intensity" />
            </Expression>
            <Expression xsi:type="GroupWorkflow">
              <Name>LEDboardSettings</Name>
              <Description>COM Port — Define the serial port used to communicate with the LED board.
⚠ Unique to each setup (check OS device list).
♫ If wrong, stimuli won’t trigger.

Infrared Intensity — Set IR level (a.u. 1–120).
🪰 100</Description>
              <Workflow>
                <Nodes>
                  <Expression xsi:type="rx:BehaviorSubject" TypeArguments="harp:HarpMessage">
                    <rx:Name>LedBoardCommands</rx:Name>
                  </Expression>
                  <Expression xsi:type="ExternalizedMapping">
                    <Property Name="PortName" DisplayName="⚠ LED - COM Port" Description="Serial communication port used by the LED array controller.&#xD;&#xA;⚠ Must match the actual COM port of the LED board.&#xD;&#xA;♫ If wrong, the LED won’t respond.&#xD;&#xA;♫ Typical format: COM7" Category="1. Hardware Config" />
                  </Expression>
                  <Expression xsi:type="Combinator">
                    <Combinator xsi:type="harp:Device">
                      <harp:OperationMode>Active</harp:OperationMode>
                      <harp:OperationLed>Off</harp:OperationLed>
                      <harp:DumpRegisters>true</harp:DumpRegisters>
                      <harp:VisualIndicators>Off</harp:VisualIndicators>
                      <harp:Heartbeat>Disabled</harp:Heartbeat>
                      <harp:IgnoreErrors>false</harp:IgnoreErrors>
                      <harp:PortName>{{@dropdown COMn}}</harp:PortName>
                    </Combinator>
                  </Expression>
                  <Expression xsi:type="rx:PublishSubject">
                    <Name>LedBoardEvents</Name>
                  </Expression>
                  <Expression xsi:type="WorkflowOutput" />
                  <Expression xsi:type="ExternalizedMapping">
                    <Property Name="Value" DisplayName="LED - IR Intensity" Description="Sets the infrared LED brightness in arbitrary units (1–120).&#xD;&#xA;🪰 100 a.u.&#xD;&#xA;♫ Adjust only if hardware differs; Tracking is sensitive to changes." Category="1. Hardware Config" />
                  </Expression>
                  <Expression xsi:type="Combinator">
                    <Combinator xsi:type="IntProperty">
                      <Value>100</Value>
                    </Combinator>
                  </Expression>
                  <Expression xsi:type="p1:LedArrayCommand">
                    <p1:Type>WriteAuxiliaryIntensity</p1:Type>
                    <p1:Mask>Index0</p1:Mask>
                  </Expression>
                  <Expression xsi:type="Combinator">
                    <Combinator xsi:type="BooleanProperty">
                      <Value>true</Value>
                    </Combinator>
                  </Expression>
                  <Expression xsi:type="p1:LedArrayCommand">
                    <p1:Type>SetAuxiliaryOutput</p1:Type>
                    <p1:Mask>Index1</p1:Mask>
                  </Expression>
                  <Expression xsi:type="Combinator">
                    <Combinator xsi:type="rx:Delay">
                      <rx:DueTime>PT0.01S</rx:DueTime>
                    </Combinator>
                  </Expression>
                  <Expression xsi:type="Combinator">
                    <Combinator xsi:type="rx:Merge" />
                  </Expression>
                  <Expression xsi:type="MulticastSubject">
                    <Name>LedBoardCommands</Name>
                  </Expression>
                </Nodes>
                <Edges>
                  <Edge From="0" To="2" Label="Source1" />
                  <Edge From="1" To="2" Label="Source2" />
                  <Edge From="2" To="3" Label="Source1" />
                  <Edge From="3" To="4" Label="Source1" />
                  <Edge From="5" To="6" Label="Source1" />
                  <Edge From="6" To="7" Label="Source1" />
                  <Edge From="7" To="11" Label="Source1" />
                  <Edge From="8" To="9" Label="Source1" />
                  <Edge From="9" To="10" Label="Source1" />
                  <Edge From="10" To="11" Label="Source2" />
                  <Edge From="11" To="12" Label="Source1" />
                </Edges>
              </Workflow>
            </Expression>
            <Expression xsi:type="ExternalizedMapping">
              <Property Name="⚠ Cam - SerialNumber" />
            </Expression>
            <Expression xsi:type="GroupWorkflow">
              <Name>CameraSettings</Name>
              <Description>Camera Identification — Cameras (“Door” and “Back”) are bound by serial number.
⚠ Serial numbers are unique to each camera.
♫ If swapped, views will be wrong, check before recording.</Description>
              <Workflow>
                <Nodes>
                  <Expression xsi:type="ExternalizedMapping">
                    <Property Name="SerialNumber" DisplayName="⚠ Cam - SerialNumber" Description="Unique Camera SN used to bind the camera. &#xD;&#xA;⚠ Must match the actual serial number of the device. &#xD;&#xA;♫ If wrong, Bonsai may bind the wrong camera or fail to connect. &#xD;&#xA;♫ Verify before recording, mis-assigned IDs will swap arenas. &#xD;&#xA;" Category="1. Hardware Config" />
                  </Expression>
                  <Expression xsi:type="Combinator">
                    <Combinator xsi:type="spk:SpinnakerCapture">
                      <spk:Index xsi:nil="true" />
                      <spk:SerialNumber>{{@dropdown Camera SN}}</spk:SerialNumber>
                      <spk:ColorProcessing>Default</spk:ColorProcessing>
                    </Combinator>
                  </Expression>
                  <Expression xsi:type="scr:ExpressionTransform">
                    <scr:Name>Rename</scr:Name>
                    <scr:Expression>new(
It.Image as Image,
It.ChunkData.ExposureEndLineStatusAll as GPIO,
It.ChunkData.FrameID as FrameID,
It.ChunkData.Timestamp as Timestamp
)</scr:Expression>
                  </Expression>
                  <Expression xsi:type="rx:PublishSubject">
                    <Name>Cameras</Name>
                  </Expression>
                  <Expression xsi:type="WorkflowOutput" />
                </Nodes>
                <Edges>
                  <Edge From="0" To="1" Label="Source1" />
                  <Edge From="1" To="2" Label="Source1" />
                  <Edge From="2" To="3" Label="Source1" />
                  <Edge From="3" To="4" Label="Source1" />
                </Edges>
              </Workflow>
            </Expression>
            <Expression xsi:type="ExternalizedMapping">
              <Property Name="⚠ Stimulation Screen" />
              <Property Name="Background color" />
              <Property Name="Monitor Refresh Rate" />
              <Property Name="Monitor Width" />
              <Property Name="Monitor Height" />
              <Property Name="Color Logic" />
              <Property Name="Square Y Position" />
              <Property Name="Square X Position" />
              <Property Name="Square Width" />
              <Property Name="Square Height" />
            </Expression>
            <Expression xsi:type="GroupWorkflow">
              <Name>ScreenSettings</Name>
              <Description>Stimulation Screen — Select the display used for visual stimuli.
⚠ Unique per setup (monitor ID/position).

Photodiode Properties — Color logic, size, and position of the photodiode patch.
♫ Used for video-stimuli alignment; ensure it’s visible to the diode.

Stimulation Screen Settings — Default color, size, and render frequency.
♫ If stimulus background RGBA = 0, this setting defines the visible background.</Description>
              <Workflow>
                <Nodes>
                  <Expression xsi:type="ExternalizedMapping">
                    <Property Name="DisplayDevice" DisplayName="⚠ Stimulation Screen" Description="Monitor/display used for stimulus presentation.&#xD;&#xA;⚠ Must match the correct display ID on each setup (e.g. First).&#xD;&#xA;♫ If wrong, stimuli will appear on the wrong screen." Category="1. Hardware Config" />
                    <Property Name="ClearColor" DisplayName="Background color" Description="Default background color for the stimulus window.&#xD;&#xA;♫ If stimulus schema RGBA values are set to 0, the screen’s default fill color will show." Category="Stimulation Screen Settings" />
                    <Property Name="TargetRenderFrequency" DisplayName="Monitor Refresh Rate" Description="Refresh frequency (Hz) for rendering stimuli.&#xD;&#xA;🪰 240 Hz&#xD;&#xA;⚠ Must match the physical monitor refresh rate (e.g. 240 Hz).&#xD;&#xA;♫ Mismatched frequencies can cause frame judder or timing inaccuracies." Category="Stimulation Screen Settings" />
                    <Property Name="Width" DisplayName="Monitor Width" Description="Width of the stimulation window in pixels.&#xD;&#xA;🪰 1920 px&#xD;&#xA;⚠ Must match the correct display resolution.&#xD;&#xA;♫ Mismatched size will cause unexpected stimulus size rendering." Category="Stimulation Screen Settings" />
                    <Property Name="Height" DisplayName="Monitor Height" Description="Height of the stimulation window in pixels.&#xD;&#xA;🪰 1080 px&#xD;&#xA;⚠ Must match the correct display resolution.&#xD;&#xA;♫ Mismatched size will cause unexpected stimulus size rendering." Category="Stimulation Screen Settings" />
                  </Expression>
                  <Expression xsi:type="Combinator">
                    <Combinator xsi:type="gl:CreateWindow">
                      <gl:Width>1920</gl:Width>
                      <gl:Height>1080</gl:Height>
                      <gl:VSync>On</gl:VSync>
                      <gl:SwapSync>true</gl:SwapSync>
                      <gl:ClearColor>White</gl:ClearColor>
                      <gl:ClearMask>DepthBufferBit ColorBufferBit</gl:ClearMask>
                      <gl:CursorVisible>false</gl:CursorVisible>
                      <gl:Location xsi:nil="true" />
                      <gl:WindowBorder>Hidden</gl:WindowBorder>
                      <gl:WindowState>Maximized</gl:WindowState>
                      <gl:DisplayDevice>First</gl:DisplayDevice>
                      <gl:TargetRenderFrequency>240</gl:TargetRenderFrequency>
                      <gl:TargetUpdateFrequency xsi:nil="true" />
                      <gl:RenderState />
                      <gl:GraphicsMode>
                        <gl:ColorFormat>
                          <gl:Red>8</gl:Red>
                          <gl:Green>8</gl:Green>
                          <gl:Blue>8</gl:Blue>
                          <gl:Alpha>8</gl:Alpha>
                        </gl:ColorFormat>
                        <gl:Depth>16</gl:Depth>
                        <gl:Stencil>0</gl:Stencil>
                        <gl:Samples>0</gl:Samples>
                        <gl:AccumulatorFormat>
                          <gl:Red>0</gl:Red>
                          <gl:Green>0</gl:Green>
                          <gl:Blue>0</gl:Blue>
                          <gl:Alpha>0</gl:Alpha>
                        </gl:AccumulatorFormat>
                        <gl:Buffers>2</gl:Buffers>
                        <gl:Stereo>false</gl:Stereo>
                      </gl:GraphicsMode>
                    </Combinator>
                  </Expression>
                  <Expression xsi:type="IncludeWorkflow" Path="BonVision:Primitives.BonVisionResources.bonsai" />
                  <Expression xsi:type="IncludeWorkflow" Path="Extensions\BonFlyResources.bonsai" />
                  <Expression xsi:type="Combinator">
                    <Combinator xsi:type="res:LoadResources" />
                  </Expression>
                  <Expression xsi:type="Combinator">
                    <Combinator xsi:type="gl:RenderFrame" />
                  </Expression>
                  <Expression xsi:type="IncludeWorkflow" Path="Extensions\BonFlyView.bonsai">
                    <Left>-27.185</Left>
                    <Right>27.185</Right>
                    <Bottom>-15.13</Bottom>
                    <Top>15.13</Top>
                  </Expression>
                  <Expression xsi:type="rx:PublishSubject">
                    <Name>Draw</Name>
                  </Expression>
                  <Expression xsi:type="Combinator">
                    <Combinator xsi:type="gl:RenderFrame" />
                  </Expression>
                  <Expression xsi:type="Combinator">
                    <Combinator xsi:type="gl:CreateOrthographicOffCenter">
                      <gl:Left>-1</gl:Left>
                      <gl:Right>1</gl:Right>
                      <gl:Bottom>-1</gl:Bottom>
                      <gl:Top>1</gl:Top>
                      <gl:NearClip>-1000</gl:NearClip>
                      <gl:FarClip>1000</gl:FarClip>
                    </Combinator>
                  </Expression>
                  <Expression xsi:type="Combinator">
                    <Combinator xsi:type="rx:Take">
                      <rx:Count>1</rx:Count>
                    </Combinator>
                  </Expression>
                  <Expression xsi:type="rx:AsyncSubject">
                    <Name>BackgroundView</Name>
                  </Expression>
                  <Expression xsi:type="Combinator">
                    <Combinator xsi:type="BooleanProperty">
                      <Value>false</Value>
                    </Combinator>
                  </Expression>
                  <Expression xsi:type="rx:BehaviorSubject">
                    <Name>SyncState</Name>
                  </Expression>
                  <Expression xsi:type="SubscribeSubject">
                    <Name>Draw</Name>
                  </Expression>
                  <Expression xsi:type="SubscribeSubject">
                    <Name>SyncState</Name>
                  </Expression>
                  <Expression xsi:type="ExternalizedMapping">
                    <Property Name="Expression" DisplayName="Color Logic" Description="Defines ON/OFF color values for the photodiode square, used for video-stimulus synchronization.&#xD;&#xA;🪰 it ? 0 : 1&#xD;&#xA;i.e. when stimulus is ON, square is black, else white." Category="Photodiode Properties" />
                  </Expression>
                  <Expression xsi:type="scr:ExpressionTransform">
                    <scr:Expression>it ? 0 : 1</scr:Expression>
                  </Expression>
                  <Expression xsi:type="PropertyMapping">
                    <PropertyMappings>
                      <Property Name="ColorR" />
                      <Property Name="ColorG" />
                      <Property Name="ColorB" />
                    </PropertyMappings>
                  </Expression>
                  <Expression xsi:type="ExternalizedMapping">
                    <Property Name="Value" DisplayName="Square Y Position" Description="Vertical offset of the photodiode square.&#xD;&#xA;Normalized -1(bottom) to 1(top) coordinates.&#xD;&#xA;🪰 1, i.e. top-right corner." Category="Photodiode Properties" />
                  </Expression>
                  <Expression xsi:type="Combinator">
                    <Combinator xsi:type="FloatProperty">
                      <Value>1</Value>
                    </Combinator>
                  </Expression>
                  <Expression xsi:type="scr:ExpressionTransform">
                    <scr:Expression>it*13.9</scr:Expression>
                  </Expression>
                  <Expression xsi:type="PropertyMapping">
                    <PropertyMappings>
                      <Property Name="LocationY" />
                    </PropertyMappings>
                  </Expression>
                  <Expression xsi:type="ExternalizedMapping">
                    <Property Name="Value" DisplayName="Square X Position" Description="Horizontal offset of the photodiode square.&#xD;&#xA;Normalized -1(left) to 1(right) coordinates.&#xD;&#xA;🪰 1; i.e. top-right corner." Category="Photodiode Properties" />
                  </Expression>
                  <Expression xsi:type="Combinator">
                    <Combinator xsi:type="FloatProperty">
                      <Value>1</Value>
                    </Combinator>
                  </Expression>
                  <Expression xsi:type="scr:ExpressionTransform">
                    <scr:Expression>it*26</scr:Expression>
                  </Expression>
                  <Expression xsi:type="PropertyMapping">
                    <PropertyMappings>
                      <Property Name="LocationX" />
                    </PropertyMappings>
                  </Expression>
                  <Expression xsi:type="ExternalizedMapping">
                    <Property Name="ExtentX" DisplayName="Square Width" Description="Width of the photodiode square.&#xD;&#xA;🪰 2&#xD;&#xA;♫ Should be large enough for reliable diode detection but small enough not to occupy too much the monitor." Category="Photodiode Properties" />
                    <Property Name="ExtentY" DisplayName="Square Height" Description="Width of the photodiode square.&#xD;&#xA;🪰 2&#xD;&#xA;♫ Should be large enough for reliable diode detection but small enough not to occupy too much the monitor." Category="Photodiode Properties" />
                  </Expression>
                  <Expression xsi:type="IncludeWorkflow" Path="BonVision:Primitives.DrawQuad.bonsai">
                    <ExtentX>2</ExtentX>
                    <ExtentY>2</ExtentY>
                    <LocationX>26</LocationX>
                    <LocationY>13.9</LocationY>
                    <Layer>0</Layer>
                    <Angle>0</Angle>
                    <ColorR>1</ColorR>
                    <ColorG>1</ColorG>
                    <ColorB>1</ColorB>
                    <ColorA>1</ColorA>
                  </Expression>
                </Nodes>
                <Edges>
                  <Edge From="0" To="1" Label="Source1" />
                  <Edge From="1" To="2" Label="Source1" />
                  <Edge From="2" To="3" Label="Source1" />
                  <Edge From="3" To="4" Label="Source1" />
                  <Edge From="5" To="6" Label="Source1" />
                  <Edge From="6" To="7" Label="Source1" />
                  <Edge From="8" To="9" Label="Source1" />
                  <Edge From="9" To="10" Label="Source1" />
                  <Edge From="10" To="11" Label="Source1" />
                  <Edge From="12" To="13" Label="Source1" />
                  <Edge From="14" To="28" Label="Source1" />
                  <Edge From="15" To="17" Label="Source1" />
                  <Edge From="16" To="17" Label="Source2" />
                  <Edge From="17" To="18" Label="Source1" />
                  <Edge From="18" To="28" Label="Source2" />
                  <Edge From="19" To="20" Label="Source1" />
                  <Edge From="20" To="21" Label="Source1" />
                  <Edge From="21" To="22" Label="Source1" />
                  <Edge From="22" To="28" Label="Source3" />
                  <Edge From="23" To="24" Label="Source1" />
                  <Edge From="24" To="25" Label="Source1" />
                  <Edge From="25" To="26" Label="Source1" />
                  <Edge From="26" To="28" Label="Source4" />
                  <Edge From="27" To="28" Label="Source5" />
                </Edges>
              </Workflow>
            </Expression>
            <Expression xsi:type="ExternalizedMapping">
              <Property Name="⚠ Schema - OptoStim" />
              <Property Name="Schema - VisualStim" />
              <Property Name=".Experiment Duration" />
              <Property Name=".Sampling Rate" />
              <Property Name=".BufferVideo" />
            </Expression>
            <Expression xsi:type="GroupWorkflow">
              <Name>ExperimentSettings</Name>
              <Description>Experimental Parameters — Stimulus schema, total duration, and sampling rate.
⚠ OptoSchema is unique to each setup.
⚠ SamplingRate = expected camera FPS; doesn't affect camera config.</Description>
              <Workflow>
                <Nodes>
                  <Expression xsi:type="ExternalizedMapping">
                    <Property Name="Value" DisplayName=".BufferVideo" Description="Adds time, in seconds, both at the start and end of the experiment duration.&#xD;&#xA;🪰 5 sec&#xD;&#xA;♫ Recording time = ExperimentDuration * SamplingRate + (BufferVideo * 2)" Category="2. Experiment Design" />
                  </Expression>
                  <Expression xsi:type="Combinator">
                    <Combinator xsi:type="IntProperty">
                      <Value>5</Value>
                    </Combinator>
                  </Expression>
                  <Expression xsi:type="Combinator">
                    <Combinator xsi:type="rx:Take">
                      <rx:Count>1</rx:Count>
                    </Combinator>
                  </Expression>
                  <Expression xsi:type="rx:BehaviorSubject">
                    <Name>BufferVideo</Name>
                  </Expression>
                  <Expression xsi:type="ExternalizedMapping">
                    <Property Name="Path" DisplayName="⚠ Schema - OptoStim" Description="Schema design for optogenetic stimulation.&#xD;&#xA;⚠ If not used in experiment, select: Schemas\Repo\NoStimulus.yml&#xD;&#xA;⚠ Unique to each setup; Intensity values vary.&#xD;&#xA;♫ Check Schemas\Repo folder for commonly used protocols.&#xD;&#xA;♫ Check Schemas\Examples\ExampleOptoStim.yml for example and tutorial stimulus design." Category="2. Experiment Design" />
                  </Expression>
                  <Expression xsi:type="Combinator">
                    <Combinator xsi:type="p2:LoadSettings">
                      <p2:Path>Schemas\Repo\NoStimulus.yml</p2:Path>
                    </Combinator>
                  </Expression>
                  <Expression xsi:type="rx:AsyncSubject">
                    <Name>OptoStimuliSettings</Name>
                  </Expression>
                  <Expression xsi:type="ExternalizedMapping">
                    <Property Name="Path" DisplayName="Schema - VisualStim" Description="Schema for visual stimulus presentation.&#xD;&#xA;⚠ If not used in experiment, select: Schemas\Repo\NoStimulus.yml&#xD;&#xA;♫ Shared across setups, but version mismatches can break reproducibility.&#xD;&#xA;♫ Check Schemas\Repo folder for commonly used protocols.&#xD;&#xA;♫ Check Schemas\Examples\ExampleVisualStim.yml for example and tutorial stimulus design." Category="2. Experiment Design" />
                  </Expression>
                  <Expression xsi:type="Combinator">
                    <Combinator xsi:type="p2:LoadSettings">
                      <p2:Path>Schemas\Repo\NoStimulus.yml</p2:Path>
                    </Combinator>
                  </Expression>
                  <Expression xsi:type="rx:AsyncSubject">
                    <Name>VisualStimuliSettings</Name>
                  </Expression>
                  <Expression xsi:type="ExternalizedMapping">
                    <Property Name="Value" DisplayName=".Experiment Duration" Description="Total duration of the experiment (seconds).&#xD;&#xA;♫ Recording time = ExperimentDuration * SamplingRate + (BufferVideo * 2)" Category="2. Experiment Design" />
                  </Expression>
                  <Expression xsi:type="Combinator">
                    <Combinator xsi:type="IntProperty">
                      <Value>600</Value>
                    </Combinator>
                  </Expression>
                  <Expression xsi:type="SubscribeSubject">
                    <Name>BufferVideo</Name>
                  </Expression>
                  <Expression xsi:type="Multiply">
                    <Operand xsi:type="IntProperty">
                      <Value>2</Value>
                    </Operand>
                  </Expression>
                  <Expression xsi:type="PropertyMapping">
                    <PropertyMappings>
                      <Property Name="Value" />
                    </PropertyMappings>
                  </Expression>
                  <Expression xsi:type="Add">
                    <Operand xsi:type="IntProperty">
                      <Value>10</Value>
                    </Operand>
                  </Expression>
                  <Expression xsi:type="ExternalizedMapping">
                    <Property Name="Value" DisplayName=".Sampling Rate" Description="Expected camera acquisition frame rate (Hz). Used for experiment duration, and logging frame rate.&#xD;&#xA;🪰 60 Hz.&#xD;&#xA;⚠ Must match the actual camera FPS; Doesn’t change camera config.&#xD;&#xA;♫ Recording time = ExperimentDuration * SamplingRate + (BufferVideo * 2)" Category="2. Experiment Design" />
                  </Expression>
                  <Expression xsi:type="Combinator">
                    <Combinator xsi:type="IntProperty">
                      <Value>60</Value>
                    </Combinator>
                  </Expression>
                  <Expression xsi:type="rx:BehaviorSubject">
                    <Name>SamplingRate</Name>
                  </Expression>
                  <Expression xsi:type="Combinator">
                    <Combinator xsi:type="rx:Zip" />
                  </Expression>
                  <Expression xsi:type="Multiply" />
                  <Expression xsi:type="rx:BehaviorSubject">
                    <Name>ExpDuration</Name>
                  </Expression>
                </Nodes>
                <Edges>
                  <Edge From="0" To="1" Label="Source1" />
                  <Edge From="1" To="2" Label="Source1" />
                  <Edge From="2" To="3" Label="Source1" />
                  <Edge From="4" To="5" Label="Source1" />
                  <Edge From="5" To="6" Label="Source1" />
                  <Edge From="7" To="8" Label="Source1" />
                  <Edge From="8" To="9" Label="Source1" />
                  <Edge From="10" To="11" Label="Source1" />
                  <Edge From="11" To="15" Label="Source1" />
                  <Edge From="12" To="13" Label="Source1" />
                  <Edge From="13" To="14" Label="Source1" />
                  <Edge From="14" To="15" Label="Source2" />
                  <Edge From="15" To="19" Label="Source1" />
                  <Edge From="16" To="17" Label="Source1" />
                  <Edge From="17" To="18" Label="Source1" />
                  <Edge From="18" To="19" Label="Source2" />
                  <Edge From="19" To="20" Label="Source1" />
                  <Edge From="20" To="21" Label="Source1" />
                </Edges>
              </Workflow>
            </Expression>
            <Expression xsi:type="ExternalizedMapping">
              <Property Name="0. Save Folder" />
              <Property Name="1. Experimenter" />
              <Property Name="2. Genotype" />
              <Property Name="3. Protocol" />
              <Property Name="4. Sex" />
              <Property Name="5. Age" />
              <Property Name="6. FH Setup" />
            </Expression>
            <Expression xsi:type="GroupWorkflow">
              <Name>FilenameSettings</Name>
