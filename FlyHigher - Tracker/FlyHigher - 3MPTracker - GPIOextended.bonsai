                                                  <ipy:Name>Angles</ipy:Name>
                                                  <ipy:Script>
import clr
clr.AddReference("OpenCV.Net")
from System import Tuple, Math, Single
from OpenCV.Net import *
from math import sin, cos, pi


@returns(Tuple[Point2f, Point2f, float, float, float, float, float])
def process(value):
  
  view = value.Item1.Item1

  viewX = value.Item1.Item2.X
  viewXY = value.Item1.Item2
  headXY = value.Item2.Item1.Item1
  thoraxXY = value.Item2.Item2.Item1
  abdomenXY = value.Item2.Item3.Item1
  LwingXY = value.Item2.Item4.Item1
  RwingXY = value.Item2.Item5.Item1

  pointA = Point2f()
  pointB = Point2f()

  if view == 'Vertical':
    pointA = headXY
    pointB = headXY
  elif view == 'Angle':
    pointA = headXY
    pointB = abdomenXY
  else:
    pointA = viewXY
    pointB = thoraxXY


  vectorAB = pointA - pointB
  angle = Math.Atan2(vectorAB.X, vectorAB.Y)

  vectorHT = headXY - thoraxXY
  angleHT = Math.Atan2(vectorHT.X, vectorHT.Y)
  diffHT = angleHT - angle
 
  vectorTA = thoraxXY - abdomenXY
  angleTA = Math.Atan2(vectorTA.X, vectorTA.Y)
  diffTA = angleTA - angle

  vectorTL = thoraxXY - LwingXY
  angleTL = Math.Atan2(vectorTL.X, vectorTL.Y)
  diffTL = angleTL - angle

  vectorTR = thoraxXY - RwingXY
  angleTR = Math.Atan2(vectorTR.X, vectorTR.Y)
  diffTR = angleTR - angle

  output = Tuple.Create(pointA, pointB, angle, diffHT, diffTA, diffTL, diffTR)
  return (output)</ipy:Script>
