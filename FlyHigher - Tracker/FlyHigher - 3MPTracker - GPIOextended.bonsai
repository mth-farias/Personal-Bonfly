                                                  <ipy:Name>CleanView</ipy:Name>
                                                  <ipy:Script>
import clr
clr.AddReference("OpenCV.Net")
from System import Tuple, Math, Single
from OpenCV.Net import *
from math import sin, cos, pi


@returns(Tuple[str, Point2f, float])
def process(value):

  Hx = value.Item1.Item1.Item1.X
  Tx = value.Item1.Item2.Item1.X
  Ax = value.Item1.Item3.Item1.X

  visible_n = 0
  if Hx &gt; 0:
    visible_n = visible_n+1
  if Ax &gt; 0:
    visible_n= visible_n+1
  if Tx &gt; 0:
    visible_n= visible_n+1



  bestX = value.Item2.Item2.X
  bestY = value.Item2.Item2.Y


  conf = float()

  view = str()
  if bestX &gt; 0:
    if visible_n == 3:
      view = value.Item2.Item1    
      conf = value.Item2.Item3
    else:
      view = 'Angle'
      conf = 1
  if visible_n == 1:
    view = 'Vertical'
    conf = 1
  if visible_n == 2:
    view = 'Angle'
    conf = 1

  output = Tuple.Create(view, Point2f(bestX, bestY), float(conf))
  return (output)</ipy:Script>
