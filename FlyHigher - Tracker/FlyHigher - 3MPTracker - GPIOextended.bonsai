from OpenCV.Net import *

@returns(IplImage)
def process(value):
  output =  value.Item1.Clone()
  image = value.Item1.Clone()
  output = IplImage(image.Size, image.Depth, 3)
  CV.CvtColor(image,output,ColorConversion.Gray2Bgr)

  view = value.Item2.Item1.Item1

  angleA = value.Item2.Item2.Item1
  angleB = value.Item2.Item2.Item2
  pointAX = value.Item2.Item2.Item1.X
  pointBX = value.Item2.Item2.Item2.X
  
  head = value.Item2.Item3.Item1.Item1
  thorax = value.Item2.Item3.Item2.Item1
  abdomen = value.Item2.Item3.Item3.Item1
  Lwing = value.Item2.Item3.Item4.Item1
  Rwing = value.Item2.Item3.Item5.Item1


  if pointAX &gt; 0:
    if pointBX &gt; 0:
      CV.Line(output, Point(angleA), Point(angleB), Scalar.Rgb(255,255,255), 0.1)

  if view == 'Left':
    CV.Line(output, Point(angleA), Point(thorax), Scalar.Rgb(255,0,0), 0.1)
  if view == 'Right':
    CV.Line(output, Point(angleA), Point(thorax), Scalar.Rgb(0,255,0), 0.1)
  if view == 'Top':
    CV.Line(output, Point(angleA), Point(thorax), Scalar.Rgb(0,0,255), 0.1)
  if view == 'Bottom':
    CV.Line(output, Point(angleA), Point(thorax), Scalar.Rgb(255,255,0), 0.1)


  CV.Circle(output, Point(head), 1, Scalar.Rgb(128, 0, 255), -1)
  CV.Circle(output, Point(thorax), 1, Scalar.Rgb(191, 0, 255), -1)
  CV.Circle(output, Point(abdomen), 1, Scalar.Rgb(255, 0, 255), -1)
  CV.Circle(output, Point(Lwing), 1, Scalar.Rgb(255, 128, 0), -1)
  CV.Circle(output, Point(Rwing), 1, Scalar.Rgb(0, 255, 128), -1)

  return output</ipy:Script>
