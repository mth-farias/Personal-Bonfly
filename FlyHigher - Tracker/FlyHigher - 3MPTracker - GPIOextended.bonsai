                                                  <ipy:Name>BestView</ipy:Name>
                                                  <ipy:Script>
import clr
clr.AddReference("OpenCV.Net")
from System import Tuple, Math, Single
from OpenCV.Net import *
from math import sin, cos, pi


@returns(Tuple[str, Point2f, float])
def process(value):
  Lx = value.Item1.Item1.X
  Ly = value.Item1.Item1.Y
  Lconf = value.Item1.Item2

  Rx = value.Item2.Item1.X
  Ry = value.Item2.Item1.Y
  Rconf = value.Item2.Item2

  Tx = value.Item3.Item1.X
  Ty = value.Item3.Item1.Y
  Tconf = value.Item3.Item2

  Bx = value.Item4.Item1.X
  By = value.Item4.Item1.Y
  Bconf = value.Item4.Item2

  view = str()
  maxconf = float()
  x = float()
  y = float()

  if Lconf &gt; Rconf and Lconf &gt; Tconf and Lconf &gt; Bconf:
     view = 'Left'
     maxconf = Lconf
     x = Lx
     y = Ly
  elif Rconf &gt; Lconf and Rconf &gt; Tconf and Rconf &gt; Bconf:
     view = 'Right'
     maxconf = Rconf
     x = Rx
     y = Ry
  elif Tconf &gt; Lconf and Tconf &gt; Rconf and Tconf &gt; Bconf:
     view = 'Top'
     maxconf = Tconf
     x = Tx
     y = Ty
  elif Bconf &gt; Lconf and Bconf &gt; Rconf and Bconf &gt; Tconf:
     view = 'Bottom'
     maxconf = Bconf
     x = Bx
     y = By

  output = Tuple.Create(view, Point2f(x, y), float(maxconf))

  return (output)</ipy:Script>
