from math import sin, cos, pi

font = None
def load():
  global font
  font = Font(2,2)

@returns(IplImage)
def process(value):
  output =  value.Item1.Item2.Clone()
  image = value.Item1.Item2.Clone()
  output = IplImage(image.Size, image.Depth, 3)
  CV.CvtColor(image,output,ColorConversion.Gray2Bgr)

  LoomState = value.Item1.Item1.Item1
  RedState = value.Item1.Item1.Item2
  GreenState = value.Item1.Item1.Item3

  if RedState == 1:
    CV.Circle(output, Point(176, 176), 90, Scalar.Rgb(204, 0, 0), -1)

  if GreenState == 1:
    CV.Circle(output, Point(176, 176), 90, Scalar.Rgb(0, 204, 0), -1)

  if LoomState == 1:
    CV.Circle(output, Point(176, 176), 80, Scalar.Rgb(255, 255, 255), -1)
  else:
    CV.Circle(output, Point(176, 176), 80, Scalar.Rgb(32, 32, 32), -1)

  FrameNumber = 'Frame # ' + value.Item2.ToString()
  CV.PutText(output,FrameNumber,Point(20,330),font,Scalar.All(255))

  return output</ipy:Script>
